<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/content_frame"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1">

    <androidx.fragment.app.FragmentContainerView
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.disposisi.MapsRumahActivity"
        android:layout_weight="1"
        />


    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10dp"
        android:weightSum="0.8"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Foto Rumah"
            android:textColor="@android:color/black"
            android:textSize="18sp"
            android:fontFamily="@font/poppins_bold"
            android:layout_marginTop="10dp"
            />
        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/layout_image"
            android:fillViewport="true"
            android:scrollbars="none"

            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="110dp"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal"
                android:padding="5dp"
                android:baselineAligned="false">
                <RelativeLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">
                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="8dp"
                        android:layout_marginTop="10dp"
                        android:id="@+id/frame1"
                        >
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="wrap_content"
                            app:cardCornerRadius="10dp"
                            android:id="@+id/cv1"
                            app:strokeWidth="2dp"
                            android:layout_height="match_parent">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:src="@drawable/ic_add_gray"
                                    tools:ignore="ContentDescription" />
                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/border"
                                    android:padding="2dp"
                                    android:scaleType="fitXY"
                                    android:id="@+id/image1"
                                    tools:ignore="ContentDescription" />

                            </RelativeLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </FrameLayout>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_close"
                        android:background="@drawable/bg_circle"
                        android:id="@+id/del1"
                        android:layout_alignParentEnd="true"
                        tools:ignore="ContentDescription" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="90dp"
                    android:layout_marginEnd="5dp"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">
                    <FrameLayout
                        android:layout_width="88dp"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="8dp"
                        android:layout_marginTop="10dp"
                        >
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            android:id="@+id/cv2"
                            app:cardCornerRadius="10dp"
                            app:strokeWidth="2dp"
                            android:elevation="0dp"
                            android:layout_height="match_parent">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:src="@drawable/ic_add_gray"
                                    tools:ignore="ContentDescription" />
                                <ImageView
                                    android:padding="2dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/border"
                                    android:scaleType="fitXY"
                                    android:id="@+id/image2"
                                    tools:ignore="ContentDescription" />

                            </RelativeLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </FrameLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_close"
                        android:background="@drawable/bg_circle"
                        android:id="@+id/del2"
                        android:layout_alignParentEnd="true"
                        tools:ignore="ContentDescription" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">
                    <FrameLayout
                        android:layout_marginEnd="8dp"
                        android:layout_width="88dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp">
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            app:cardCornerRadius="10dp"
                            app:strokeWidth="2dp"
                            android:id="@+id/cv3"
                            android:elevation="0dp"
                            android:layout_height="match_parent">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:src="@drawable/ic_add_gray"
                                    tools:ignore="ContentDescription" />
                                <ImageView
                                    android:padding="2dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/border"
                                    android:scaleType="fitXY"
                                    android:id="@+id/image3"
                                    tools:ignore="ContentDescription" />

                            </RelativeLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </FrameLayout>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_close"
                        android:background="@drawable/bg_circle"
                        android:id="@+id/del3"
                        android:layout_alignParentEnd="true"
                        tools:ignore="ContentDescription" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">
                    <FrameLayout
                        android:layout_marginEnd="8dp"
                        android:layout_width="88dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp">
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            app:cardCornerRadius="10dp"
                            android:elevation="0dp"
                            android:id="@+id/cv4"
                            app:strokeWidth="2dp"
                            android:layout_height="match_parent">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:src="@drawable/ic_add_gray"
                                    tools:ignore="ContentDescription" />
                                <ImageView
                                    android:padding="2dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/border"
                                    android:scaleType="fitXY"
                                    android:id="@+id/image4"
                                    tools:ignore="ContentDescription" />
                            </RelativeLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </FrameLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_close"
                        android:background="@drawable/bg_circle"
                        android:id="@+id/del4"
                        android:layout_alignParentEnd="true"
                        tools:ignore="ContentDescription" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">
                    <FrameLayout
                        android:layout_marginEnd="8dp"
                        android:layout_width="88dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp">
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            app:cardCornerRadius="10dp"
                            android:elevation="0dp"
                            android:id="@+id/cv5"
                            app:strokeWidth="2dp"
                            android:layout_height="match_parent">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:src="@drawable/ic_add_gray"
                                    tools:ignore="ContentDescription" />
                                <ImageView
                                    android:padding="2dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/border"
                                    android:scaleType="fitXY"
                                    android:id="@+id/image5"
                                    tools:ignore="ContentDescription" />
                            </RelativeLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </FrameLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_close"
                        android:background="@drawable/bg_circle"
                        android:id="@+id/del5"
                        android:layout_alignParentEnd="true"
                        tools:ignore="ContentDescription" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">
                    <FrameLayout
                        android:layout_marginEnd="8dp"
                        android:layout_width="88dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp">
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            app:cardCornerRadius="10dp"
                            android:elevation="0dp"
                            android:id="@+id/cv6"
                            app:strokeWidth="2dp"
                            android:layout_height="match_parent">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:src="@drawable/ic_add_gray"
                                    tools:ignore="ContentDescription" />
                                <ImageView
                                    android:padding="2dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/border"
                                    android:scaleType="fitXY"
                                    android:id="@+id/image6"
                                    tools:ignore="ContentDescription" />
                            </RelativeLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </FrameLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_close"
                        android:background="@drawable/bg_circle"
                        android:id="@+id/del6"
                        android:layout_alignParentEnd="true"
                        tools:ignore="ContentDescription" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">
                    <FrameLayout
                        android:layout_marginEnd="8dp"
                        android:layout_width="88dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp">
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            app:cardCornerRadius="10dp"
                            android:elevation="0dp"
                            android:id="@+id/cv7"
                            app:strokeWidth="2dp"
                            android:layout_height="match_parent">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:src="@drawable/ic_add_gray"
                                    tools:ignore="ContentDescription" />
                                <ImageView
                                    android:padding="2dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/border"
                                    android:scaleType="fitXY"
                                    android:id="@+id/image7"
                                    tools:ignore="ContentDescription" />
                            </RelativeLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </FrameLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_close"
                        android:background="@drawable/bg_circle"
                        android:id="@+id/del7"
                        android:layout_alignParentEnd="true"
                        tools:ignore="ContentDescription" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent"
                    tools:ignore="UselessParent">
                    <FrameLayout
                        android:layout_marginEnd="8dp"
                        android:layout_width="88dp"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dp">
                        <com.google.android.material.card.MaterialCardView
                            android:layout_width="match_parent"
                            app:cardCornerRadius="10dp"
                            android:elevation="0dp"
                            android:id="@+id/cv8"
                            app:strokeWidth="2dp"
                            android:layout_height="match_parent">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                tools:ignore="UselessLeaf">
                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:src="@drawable/ic_add_gray"
                                    tools:ignore="ContentDescription" />
                                <ImageView
                                    android:padding="2dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:src="@drawable/border"
                                    android:scaleType="fitXY"
                                    android:id="@+id/image8"
                                    tools:ignore="ContentDescription" />
                            </RelativeLayout>
                        </com.google.android.material.card.MaterialCardView>
                    </FrameLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_close"
                        android:background="@drawable/bg_circle"
                        android:id="@+id/del8"
                        android:layout_alignParentEnd="true"
                        tools:ignore="ContentDescription" />
                </RelativeLayout>

            </LinearLayout>
        </HorizontalScrollView>
        <View
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:background="#2367809F"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/lokasi_anda_saat_ini"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:fontFamily="@font/poppins_bold"
            />
        <TextView
            android:id="@+id/tvLocations"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Street hlorem ipsum"
            android:fontFamily="@font/poppins_semibold"
            />
        <com.google.android.material.button.MaterialButton
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cornerRadius="5dp"
            android:id="@+id/btnLocation"
            android:text="@string/pilih_lokasi"
            />
    </androidx.appcompat.widget.LinearLayoutCompat>


</androidx.appcompat.widget.LinearLayoutCompat>
